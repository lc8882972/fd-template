  },
  "scaffoldConfig": {
    "type": "react",
    "name": "@alifd/template-next-single-page",
    "title": "template-next-single-page",
    "screenshot": "https://img.alicdn.com/tfs/TB1ntFrq5rpK1RjSZFhXXXSdXXa-2710-1638.png",
