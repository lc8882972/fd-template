{
  "templateType": "custom",
  "title": "fd-template",
  "name": "@alifd/template-next-single-page",
  "description": "支持 fusion主题配置的默认模板",
  "version": "1.0.0",
  "license": "MIT",
  "keywords": [
    "next",
    "fusion",
    "pc",
    "next",
    "ice-scaffold"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/lc8882972/fd-template"
  },
  "publishConfig": {
    "registry": "http://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "start": "cross-env DEV=1 LIVELOAD=1 webpack-dev-server --mode development --config ./webpack.config.dev.js --host 0.0.0.0 --port 8090",
    "build": "rimraf dist && webpack --mode production --config ./webpack.config.prod.js",
    "server": "cross-env port=9000 node server.js",
    "lint": "tslint --fix --project ./tsconfig.json",
    "prettier": "prettier src/**/*.scss --write"
  },
  "lint-staged": {
    "src/**/*.@(ts|tsx)": [
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run prettier && lint-staged"
    }
  },
  "author": {
    "name": "Yan",
    "email": "410115424@qq.com"
  },
  "maintainers": [
    {
      "name": "Yan",
      "email": "410115424@qq.com"
    }
  ],
  "readmeFilename": "README.md",
  "tnpm": {
    "mode": "npminstall"
  },
  "dependencies": {
    "@alifd/next": "^1.x",
    "@alifd/theme-2": "*",
    "@babel/runtime": "^7.4.4",
